<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Reports::report">
                    <resource id="Fredden_JavaScriptErrorReporting::group" title="JavaScript Error Reporting" sortOrder="10" >
                        <resource id="Fredden_JavaScriptErrorReporting::view" title="View" sortOrder="150" />
                        <resource id="Fredden_JavaScriptErrorReporting::delete" title="Delete" sortOrder="200" />
                        <resource id="Fredden_JavaScriptErrorReporting::ignore" title="Manage list of ignored errors" sortOrder="250" />
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
